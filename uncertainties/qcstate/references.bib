  keywords = {Physics - Computational Physics,Quantum Physics},
  file = {C\:\\Users\\sleet\\Zotero\\storage\\Y492N946\\Pan et al. - 2022 - Solving the sampling problem of the Sycamore quant.pdf;C\:\\Users\\sleet\\Zotero\\storage\\Z7B74H9G\\Pan et al. - 2022 - Solving the sampling problem of the Sycamore quant.pdf;C\:\\Users\\sleet\\Zotero\\storage\\BDGGPJGS\\2111.html;C\:\\Users\\sleet\\Zotero\\storage\\BEY33HAN\\PhysRevLett.129.html}
