  keywords = {Physics - Chemical Physics,Quantum Physics},
  file = {C\:\\Users\\sleet\\Zotero\\storage\\VL3FZWF5\\Arute et al. - 2020 - Hartree-Fock on a superconducting qubit quantum co.pdf;C\:\\Users\\sleet\\Zotero\\storage\\JTFQNS7X\\2004.html}
