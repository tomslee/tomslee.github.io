	file = {10.1080@01442872.2020.1748264.pdf:C\:\\Users\\sleet\\Zotero\\storage\\KEFE6AK5\\10.1080@01442872.2020.1748264.pdf:application/pdf;Full Text PDF:C\:\\Users\\sleet\\Zotero\\storage\\KEHDXIUE\\Birch et al. - 2020 - The problem of innovation in technoscientific capi.pdf:application/pdf;Snapshot:C\:\\Users\\sleet\\Zotero\\storage\\VL3SW7G5\\01442872.2020.html:text/html},
