	author = {Yan, Chiwei and Zhu, Helin and Korolko, Nikita and Woodard, Dawn},
	year = {2020},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/nav.21872},
	keywords = {dynamic pricing, dynamic waiting, matching, ride-hailing, ride-sharing},
	pages = {705--724},
	file = {Snapshot:C\:\\Users\\sleet\\Zotero\\storage\\RV8G9GYX\\nav.html:text/html;Yan et al. - 2020 - Dynamic pricing and matching in ride-hailing platf.pdf:C\:\\Users\\sleet\\Zotero\\storage\\8RNR7XMJ\\Yan et al. - 2020 - Dynamic pricing and matching in ride-hailing platf.pdf:application/pdf},
