@article{liRegulatingTNCsShould2019,
  title = {Regulating {{TNCs}}: {{Should Uber}} and {{Lyft}} Set Their Own Rules?},
  shorttitle = {Regulating {{TNCs}}},
  author = {Li, Sen and Tavafoghi, Hamidreza and Poolla, Kameshwar and Varaiya, Pravin},
  year = {2019},
