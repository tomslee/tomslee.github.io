@article{gillerCorrelatesModalSubstitution2024,
  title = {Correlates of {{Modal Substitution}} and {{Induced Travel}} of {{Ridehailing}} in {{California}}},
  author = {Giller, James and Young, Mischa and Circella, Giovanni},
