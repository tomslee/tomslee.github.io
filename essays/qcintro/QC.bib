	keywords = {Quantum Physics, Condensed Matter - Materials Science, Condensed Matter - Strongly Correlated Electrons},
	file = {arXiv Fulltext PDF:C\:\\Users\\sleet\\Zotero\\storage\\TZVXZ6Q8\\Clinton et al. - 2022 - Towards near-term quantum simulation of materials.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\sleet\\Zotero\\storage\\5DYA6PHA\\2205.html:text/html},
