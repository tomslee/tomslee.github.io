	note = {arXiv: 1812.09976},
	keywords = {Quantum Physics},
	pages = {10856--10915},
	file = {arXiv Fulltext PDF:C\:\\Users\\sleet\\Zotero\\storage\\9NFFHCWB\\Cao et al. - 2019 - Quantum Chemistry in the Age of Quantum Computing.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\sleet\\Zotero\\storage\\URUM6J2W\\1812.html:text/html},
